# Add this to your .env.local file
# You can get these values from your Supabase dashboard

# Public Supabase URL (already exists)
NEXT_PUBLIC_SUPABASE_URL=your_supabase_project_url

# Public anon key (already exists) 
NEXT_PUBLIC_SUPABASE_ANON_KEY=your_supabase_anon_key

# Service role key (needed for user management API)
# ⚠️ IMPORTANT: This is a SECRET key - never expose in client-side code
# Get this from Supabase Dashboard > Settings > API > Service Role Key
SUPABASE_SERVICE_ROLE_KEY=your_supabase_service_role_key

# CCB API Configuration for Event Notes Integration
# These credentials should be obtained from your CCB administrator
CCB_BASE_URL=https://valleycreekchurch.ccbchurch.com/api.php
CCB_API_USER=circlesreportingapi
CCB_API_PASSWORD=your_ccb_api_password
